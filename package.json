    "test:unit": "pnpm -r test:unit"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "packageManager": "pnpm@8.15.1"
}
